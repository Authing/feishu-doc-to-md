{
  "name": "飞书 Markdown 导出工具",
  "version": "1.0",
  "description": "支持将飞书文档的内容以 Markdown 格式导出",
  "content_scripts": [
    {
      "matches": [
        "https://*.feishu.cn/docs/*"
      ],
      "js": [
        "js/feishu.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "飞书 Markdown 导出工具",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}